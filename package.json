{
  "name": "soundwaves",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "build:gh-pages": "cross-env GH_PAGES=true vue-cli-service build",
    "test": "vue-cli-service test",
    "e2e": "vue-cli-service e2e",
    "lint": "vue-cli-service lint",
    "e2e:open": "vue-cli-service e2e:open"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.40",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.40",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "jade": "^1.11.0",
    "lodash.flattendeep": "^4.4.0",
    "lodash.shuffle": "^4.2.0",
    "moment": "^2.20.1",
    "vue": "^2.5.13",
    "vue-class-component": "^6.2.0",
    "vue-material": "^1.0.0-beta-7",
    "vue-property-decorator": "^6.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.0",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-alpha.5",
    "@vue/cli-plugin-e2e-cypress": "^3.0.0-alpha.5",
    "@vue/cli-plugin-eslint": "^3.0.0-alpha.5",
    "@vue/cli-plugin-unit-mocha": "^3.0.0-alpha.5",
    "@vue/cli-service": "^3.0.0-alpha.5",
    "@vue/eslint-config-prettier": "^3.0.0-alpha.5",
    "@vue/test-utils": "^1.0.0-beta.10",
    "chai": "^4.1.2",
    "cross-env": "^5.1.4",
    "lodash.debounce": "^4.0.8",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-template-compiler": "^2.5.13",
    "webpack-bundle-analyzer": "^2.11.1"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:vue/essential"
    ],
    "rules": {
      "quotes": [
        "warn",
        "single"
      ]
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
